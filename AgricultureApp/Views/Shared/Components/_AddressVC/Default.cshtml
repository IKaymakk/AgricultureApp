<p class="heading-btm-w3ls">Her Türlü Bilgi Veya İsteğiniz İçin Bize Ulaşabilirsiniz</p>
@foreach (var x in Model)
{
    <div class="col-md-3 contact-left">
        <h4>Adresimiz</h4>
        <p>@x.Descp1</p>
        <p>@x.Descp2</p>
        <p>@x.Descp3</p>
        <h4>Telefon Numaramız</h4>
        <p>@x.Descp4</p>

    </div>
}
